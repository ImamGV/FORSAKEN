local repo = 'https://raw.githubusercontent.com/deividcomsono/Obsidian/main/'

local Library = loadstring(game:HttpGet(repo .. 'Library.lua'))()
local ThemeManager = loadstring(game:HttpGet(repo .. 'addons/ThemeManager.lua'))()
local SaveManager = loadstring(game:HttpGet(repo .. 'addons/SaveManager.lua'))()
local Options = Library.Options
local Toggles = Library.Toggles
Library.ShowToggleFrameInKeybinds = true 
Library.ShowCustomCursor = true
Library.NotifySide = "Right"

local Window = Library:CreateWindow({
	Title = 'Forsaken',
	Center = true,
	AutoShow = true,
	Resizable = true,
	ShowCustomCursor = true,
	NotifySide = "Right",
	TabPadding = 8,
	MenuFadeTime = 0
})

local Tabs = {
	Main = Window:AddTab('Main')
}

local _env = getgenv and getgenv() or {}
local _head = game.Players.LocalPlayer.Character.Head
local KillerSurvival = Tabs.Main:AddLeftGroupbox("Killer & Survival")
game.Players.LocalPlayer.Character.Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
if _env.NoStun == true and game.Players.LocalPlayer.Character.Humanoid.WalkSpeed < 16 then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end
end)
game.Players.LocalPlayer.CharacterAdded:Connect(function()
game.Players.LocalPlayer.Character.Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
if _env.NoStun == true and game.Players.LocalPlayer.Character.Humanoid.WalkSpeed < 16 then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end
end)
end)
KillerSurvival:AddToggle("MyToggle",{
Text = "No Stun",
Callback = function(v)
_env.NoStun = v
end})
KillerSurvival:AddDivider()
KillerSurvival:AddSlider("MySlider",{
Text = "Speed Boost",
Min = 0, Max = 3,
Default = 1, Compact = true,
Rounding = 1,
Callback = function(v)
_env.SpeedBoostValue = v
end})
_env.SpeedBoostValue = 1
KillerSurvival:AddToggle("MyToggle",{
Text = "Speed Boost",
Callback = function(v)
_env.SpeedBoost = v
while _env.SpeedBoost do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame += game.Players.LocalPlayer.Character.Humanoid.MoveDirection * _env.SpeedBoostValue
game.Players.LocalPlayer.Character.HumanoidRootPart.CanCollide = true
wait()
end
end})
KillerSurvival:AddDivider()
function GeneratorSolution()
for _,v in ipairs(workspace.Map.Ingame.Map:GetChildren()) do
if v.Name == "Generator" and (v.Main.Position - _head.Position).Magnitude < 10 then
v.Remotes.RE:FireServer()
end
end
end
KillerSurvival:AddSlider("MySlider",{
Text = "Generator Solution Delay",
Min = 1.5, Max = 3,
Default = 1.5, Compact = true,
Rounding = 1,
Callback = function(v)
_env.GeneratorSolutionDelay = v
end})
KillerSurvival:AddToggle("MyToggle",{
Text = "Auto Generator Solution",
Callback = function(v)
_env.AutoGeneratorSolution = v
while _env.AutoGeneratorSolution do
GeneratorSolution()
wait(_env.GeneratorSolutionDelay or 1.5)
end
end})

local Visual = Tabs.Main:AddRightGroupbox("Visual")
local LibESP = loadstring(game:HttpGet("https://raw.githubusercontent.com/ImamGV/Script/main/ESP"))()

Visual:AddToggle("MyToggle",{
Text = "Survival ESP",
Callback = function(v)
if v then
for _, v in ipairs(workspace.Players:GetDescendants()) do
if v.Parent.Name == "Survivors" then
LibESP:AddESP(v, "", Color3.fromRGB(0, 255, 0), 14, "Survival_ESP")
end
end
workspace.Players.DescendantAdded:Connect(function(v)
if v.Parent.Name == "Survivors" then
LibESP:AddESP(v, "", Color3.fromRGB(0, 255, 0), 14, "Survival_ESP")
end
end)
else
LibESP:Delete("Survival_ESP")
end
end})
Visual:AddToggle("MyToggle",{
Text = "Killer ESP",
Callback = function(v)
if v then
for _, v in ipairs(workspace.Players:GetDescendants()) do
if v.Parent.Name == "Killers" then
LibESP:AddESP(v, "", Color3.fromRGB(255, 0, 0), 14, "Killers_ESP")
end
end
workspace.Players.DescendantAdded:Connect(function(v)
if v.Parent.Name == "Killers" then
LibESP:AddESP(v, "", Color3.fromRGB(255, 0, 0), 14, "Killers_ESP")
end
end)
else
LibESP:Delete("Killers_ESP")
end
end})
Visual:AddToggle("MyToggle",{
Text = "Generator ESP",
Callback = function(v)
if v then
for _, v in ipairs(workspace:GetDescendants()) do
if v.Name == "Generator" then
LibESP:AddESP(v, "", Color3.fromRGB(255, 220, 0), 14, "Generator_ESP")
end
end
workspace.DescendantAdded:Connect(function(v)
if v.Name == "Generator" then
LibESP:AddESP(v, "", Color3.fromRGB(255, 220, 0), 14, "Generator_ESP")
end
end)
else
LibESP:Delete("Generator_ESP")
end
end})
Visual:AddToggle("MyToggle",{
Text = "Medkit ESP",
Callback = function(v)
if v then
for _, v in ipairs(workspace:GetDescendants()) do
if v.Name == "Medkit" then
LibESP:AddESP(v, "Medkit", Color3.fromRGB(255, 0, 0), 14, "Medkit_ESP")
end
end
workspace.DescendantAdded:Connect(function(v)
if v.Name == "Medkit" then
LibESP:AddESP(v, "Medkit", Color3.fromRGB(255, 0, 0), 14, "Medkit_ESP")
end
end)
else
LibESP:Delete("Medkit_ESP")
end
end})
Visual:AddToggle("MyToggle",{
Text = "Cola ESP",
Callback = function(v)
if v then
for _, v in ipairs(workspace:GetDescendants()) do
if v.Name == "Bloxy Cola" then
LibESP:AddESP(v, "Cola", Color3.fromRGB(0, 255, 255), 14, "BloxyCola_ESP")
end
end
workspace.DescendantAdded:Connect(function(v)
if v.Name == "BloxyCola" then
LibESP:AddESP(v, "Cola", Color3.fromRGB(0, 255, 255), 14, "BloxyCola_ESP")
end
end)
else
LibESP:Delete("BloxyCola_ESP")
end
end})
local Interact = Tabs.Main:AddLeftGroupbox("Interact")
function ClaimBloxyCola()
for _,v in ipairs(workspace.Map.Ingame:GetDescendants()) do
if v.Name == "Bloxy Cola" then
fireproximityprompt(v:FindFirstChildWhichIsA("BasePart"):FindFirstChildWhichIsA("ProximityPrompt"))
end
end
end

local MainTabbox = Tabs.Main:AddRightTabbox()
local Lighting = MainTabbox:AddTab("Lighting")
Lighting:AddSlider("MySlider",{
Text = "Brightness",
Min = 0,
Default = 0,
Max = 3,
Rounding = 1,
Compact = true,
Callback = function(v)
_env.Brightness = v
end})
Lighting:AddToggle("MyToggle",{
Text = "No Shadows",
Default = false,
Callback = function(v)
_env.GlobalShadows = v
end})
Lighting:AddToggle("MyToggle",{
Text = "No Fog",
Default = false,
Callback = function(v)
_env.NoFog = v
end})
Lighting:AddDivider()
Lighting:AddToggle("MyToggle",{
Text = "Enabled",
Default = false,
Callback = function(v)
_env.Fullbright = v
game:GetService("RunService").RenderStepped:Connect(function()
if not game.Lighting:GetAttribute("FogStart") then game.Lighting:SetAttribute("FogStart", game.Lighting.FogStart) end
if not game.Lighting:GetAttribute("FogEnd") then game.Lighting:SetAttribute("FogEnd", game.Lighting.FogEnd) end
game.Lighting.FogStart = _env.NoFog and 0 or game.Lighting:GetAttribute("FogStart")
game.Lighting.FogEnd = _env.NoFog and math.huge or game.Lighting:GetAttribute("FogEnd")
local fog = game.Lighting:FindFirstChildOfClass("Atmosphere")
if fog then
if not fog:GetAttribute("Density") then fog:SetAttribute("Density", fog.Density) end
fog.Density = _env.NoFog and 0 or fog:GetAttribute("Density")
end
if _env.Fullbright then
game.Lighting.OutdoorAmbient = Color3.new(1,1,1)
game.Lighting.Brightness = _env.Brightness or 0
game.Lighting.GlobalShadows = not _env.GlobalShadows
else
game.Lighting.OutdoorAmbient = Color3.fromRGB(55,55,55)
game.Lighting.Brightness = 0
game.Lighting.GlobalShadows = true
end
end)
end})
local Camera = MainTabbox:AddTab("Camera")
Camera:AddSlider("MySlider",{
Text = "Field Of View",
Min = 70,
Default = 70,
Max = 120,
Rounding = 1,
Compact = true,
Callback = function(v)
_G.FovValue = v
end})
Camera:AddToggle("MyToggle",{
Text = "Enabled",
Default = false,
Callback = function(v)
_G.FOV = v
game:GetService("RunService").RenderStepped:Connect(function()
if _G.FOV then
workspace.Camera.FieldOfView = _G.FovValue or 70
end
end)
end})
