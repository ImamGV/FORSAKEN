end
end
OtherESP = workspace.DescendantAdded:Connect(function(v)
if v.Name == "PizzaDeliveryRig" or v.Name == "Bunny" or v.Name == "Mafiaso1" or v.Name == "Mafiaso2" or v.Name == "Mafiaso3" then
LibESP:AddESP(v, v.Name, Color3.fromRGB(255, 10, 10), 14, "Other_ESP")
end
end)
else
OtherESP:Disconnect()
LibESP:Delete("Other_ESP")
end
end})
