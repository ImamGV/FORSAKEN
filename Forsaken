local MainTabbox = Tabs.Main:AddRightTabbox()
local Lighting = MainTabbox:AddTab("Lighting")
Lighting:AddSlider("MySlider",{
Text = "Brightness",
Min = 0,
Default = 0,
Max = 3,
Rounding = 1,
Compact = true,
Callback = function(v)
_env.Brightness = v
end})
Lighting:AddToggle("MyToggle",{
Text = "No Shadows",
Default = false,
Callback = function(v)
_env.GlobalShadows = v
end})
Lighting:AddToggle("MyToggle",{
Text = "No Fog",
Default = false,
Callback = function(v)
_env.NoFog = v
end})
Lighting:AddDivider()
Lighting:AddToggle("MyToggle",{
Text = "Enabled",
Default = false,
Callback = function(v)
_env.Fullbright = v
game:GetService("RunService").RenderStepped:Connect(function()
if not game.Lighting:GetAttribute("FogStart") then game.Lighting:SetAttribute("FogStart", game.Lighting.FogStart) end
if not game.Lighting:GetAttribute("FogEnd") then game.Lighting:SetAttribute("FogEnd", game.Lighting.FogEnd) end
game.Lighting.FogStart = _env.NoFog and 0 or game.Lighting:GetAttribute("FogStart")
game.Lighting.FogEnd = _env.NoFog and math.huge or game.Lighting:GetAttribute("FogEnd")
local fog = game.Lighting:FindFirstChildOfClass("Atmosphere")
if fog then
if not fog:GetAttribute("Density") then fog:SetAttribute("Density", fog.Density) end
fog.Density = _env.NoFog and 0 or fog:GetAttribute("Density")
end
if _env.Fullbright then
game.Lighting.OutdoorAmbient = Color3.new(1,1,1)
game.Lighting.Brightness = _env.Brightness or 0
game.Lighting.GlobalShadows = not _env.GlobalShadows
else
game.Lighting.OutdoorAmbient = Color3.fromRGB(55,55,55)
game.Lighting.Brightness = 0
game.Lighting.GlobalShadows = true
end
end)
end})
local Camera = MainTabbox:AddTab("Camera")
Camera:AddSlider("MySlider",{
Text = "Field Of View",
Min = 70,
Default = 70,
Max = 120,
Rounding = 1,
Compact = true,
Callback = function(v)
_env.FovValue = v
end})
_G.FovValue = 70
Camera:AddToggle("MyToggle",{
Text = "Enabled",
Default = false,
Callback = function(v)
_env.FOV = v
game:GetService("RunService").RenderStepped:Connect(function()
if _env.FOV then
workspace.Camera.FieldOfView = _env.FovValue
end
end)
end})
local Notify = Tabs.Main:AddLeftGroupbox("Notify")
Notify:AddToggle("MyToggle",{
Text = "Notify Killers",
Default = false,
Callback = function(v)
if v then
NotifyKillers = workspace.Players.Killers.DescendantAdded:Connect(function(v)
if v["c00lkidd"] and v.Name == "c00lkidd" then
Library:Notify("Forsaken | Warning\nThe Killers is 'c00lkidd'")
elseif v["JohnDoe"] and v.Name == "JohnDoe" then
Library:Notify("Forsaken | Warning\nThe Killers is 'John Doe'")
elseif v["1x1x1x1"] and v.Name == "1x1x1x1" then
Library:Notify("Forsaken | Warning\nThe Killers is '1x1x1x1'!")
end
end)
else
NotifyKillers:Disconnect()
end
end})
Notify:AddToggle("MyToggle",{
Text = "Notify Spike John Doe",
Default = false,
Callback = function(v)
if v then
NotifySpikeJohnDoe = workspace.Map.Ingame.DescendantAdded:Connect(function(v)
if v.Name == "Spike" then
Library:Notify("Forsaken | Warning\nBlock 'Spike (John Doe)' has spawn!")
end
end)
else
NotifySpikeJohnDoe:Disconnect()
end
end})
Notify:AddToggle("MyToggle",{
Text = "Notify Subspace Tripmine",
Default = false,
Callback = function(v)
if v then
NotifySubspaceTripmine = workspace.Map.Ingame.DescendantAdded:Connect(function(v)
if v.Name == "SubspaceTripmine" then
Library:Notify("Forsaken | Warning\nBlock 'Subspace Tripmine' has spawn!")
end
end)
else
NotifySubspaceTripmine:Disconnect()
end
end})
Notify:AddToggle("MyToggle",{
Text = "Notify Entity Killers",
Default = false,
Callback = function(v)
if v then
NotifyEntityKillers = workspace.DescendantAdded:Connect(function(v)
if v.Name == "PizzaDeliveryRig" or v.Name == "Bunny" or v.Name == "Mafiaso1" or v.Name == "Mafiaso2" or v.Name == "Mafiaso3" then
Library:Notify("Forsaken | Warning\nEntity '" .. v.Name .. "' has spawn!")
end
end)
else
NotifyEntityKillers:Disconnect()
end
end})
local Anti = Tabs.Main:AddLeftGroupbox("Anti")
Anti:AddToggle("MyToggle",{
Text = "Anti-John Doe Trail",
Default = false,
Callback = function(v)
if v then
for _,v in ipairs(workspace.Players:FindFirstChild('Killers'):FindFirstChild("JohnDoe"):GetDescendants()) do
if v.Name == "JohnDoeTrail" then
v.Trail.CanTouch = false
end
end
AntiJohnDoeTrail = workspace.Players.Killer["JohnDoe"].DescendantAdded:Connect(function(v)
if v.Name == "JohnDoeTrail" then
v.Trail.CanTouch = false
end
end)
else
AntiJohnDoeTrail:Disconnect()
for _,v in ipairs(workspace.Players:FindFirstChild('Killers'):FindFirstChild("JohnDoe"):GetDescendants()) do
if v.Name == "JohnDoeTrail" then
v.Trail.CanTouch = true
end
end
end
end})
local Interact = Tabs.Main:AddRightGroupbox("Interact")
function InteractBloxyCola()
for _,v in ipairs(workspace.Map.Ingame:GetDescendants()) do
if v.Name == "BloxyCola" and (v:FindFirstChildWhichIsA("BasePart").Position - game.Players.LocalPlayer.Character.Head.Position).Magnitude < 10 then
fireproximityprompt(v:FindFirstChildWhichIsA("BasePart").ProximityPrompt)
end
end
end
function InteractMedkit()
for _,v in ipairs(workspace.Map.Ingame:GetDescendants()) do
if v.Name == "Medkit" and (v:FindFirstChildWhichIsA("BasePart").Position - game.Players.LocalPlayer.Character.Head.Position).Magnitude < 10 then
fireproximityprompt(v:FindFirstChildWhichIsA("BasePart").ProximityPrompt)
end
end
end
Interact:AddToggle("MyToggle",{
Text = "Auto Claim Bloxy Cola",
Default = false,
Callback = function(v)
_G.AutoClaimBloxyCola = v
game:GetService("RunService").RenderStepped:Connect(function()
if _G.AutoClaimBloxyCola then
InteractBloxyCola()
end
end)
end})
Interact:AddToggle("MyToggle",{
Text = "Auto Claim Medkit",
Default = false,
Callback = function(v)
_G.AutoClaimMedkit = v
game:GetService("RunService").RenderStepped:Connect(function()
if _G.AutoClaimMedkit then
InteractMedkit()
end
end)
end})
