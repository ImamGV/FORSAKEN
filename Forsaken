local repo = 'https://raw.githubusercontent.com/deividcomsono/Obsidian/main/'

local Library = loadstring(game:HttpGet(repo .. 'Library.lua'))()
local ThemeManager = loadstring(game:HttpGet(repo .. 'addons/ThemeManager.lua'))()
local SaveManager = loadstring(game:HttpGet(repo .. 'addons/SaveManager.lua'))()
local Options = Library.Options
local Toggles = Library.Toggles
Library.ShowToggleFrameInKeybinds = true 
Library.ShowCustomCursor = true
Library.NotifySide = "Right"

local Window = Library:CreateWindow({
	Title = 'Forsaken',
	Center = true,
	AutoShow = true,
	Resizable = true,
	ShowCustomCursor = true,
	NotifySide = "Right",
	TabPadding = 8,
	MenuFadeTime = 0
})

local Tabs = {
	Main = Window:AddTab('Main')
}

local _env = getgenv and getgenv() or {}
local _hrp = game.Players.LocalPlayer.Character.HumanoidRootPart
if game.Players.LocalPlayer.Name == "rechedmcvnGV" or game.Players.LocalPlayer.Name == "rechedmcvn" then
Library:Notify("Hello, my brother")
end
local KillerSurvival = Tabs.Main:AddLeftGroupbox("Killer & Survival")
game.Players.LocalPlayer.Character.Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
if _env.NoStun == true and game.Players.LocalPlayer.Character.Humanoid.WalkSpeed < 16 then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end
end)
game.Players.LocalPlayer.CharacterAdded:Connect(function()
game.Players.LocalPlayer.Character.Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
if _env.NoStun == true and game.Players.LocalPlayer.Character.Humanoid.WalkSpeed < 16 then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end
end)
end)
KillerSurvival:AddToggle("MyToggle",{
Text = "No Stun",
Callback = function(v)
_env.NoStun = v
end})
KillerSurvival:AddDivider()
KillerSurvival:AddSlider("MySlider",{
Text = "Speed Boost",
Min = 0, Max = 3,
Default = 1, Compact = true,
Rounding = 1,
Callback = function(v)
_env.SpeedBoostValue = v
end})
_env.SpeedBoostValue = 1
KillerSurvival:AddToggle("MyToggle",{
Text = "Speed Boost",
Callback = function(v)
_env.SpeedBoost = v
while _env.SpeedBoost do
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame += game.Players.LocalPlayer.Character.Humanoid.MoveDirection * _env.SpeedBoostValue
game.Players.LocalPlayer.Character.HumanoidRootPart.CanCollide = true
task.wait()
end
end})
KillerSurvival:AddDivider()
function AutoPrizeGenerator(value)
local runscript = value
local debounce = {}
while runscript do
for _,v in ipairs(workspace.Map.Ingame.Map:GetChildren()) do
if v.Name == "Generator" and v then
v.Remotes.RE:FireServer()
end
end
wait(_env.GeneratorSolutionDelay or 1.5)
end
end
KillerSurvival:AddSlider("MySlider",{
Text = "Generator Solution Delay",
Min = 1.25, Max = 5,
Default = 1.5, Compact = true,
Rounding = 1,
Callback = function(v)
_env.GeneratorSolutionDelay = v
end})
KillerSurvival:AddToggle("MyToggle",{
Text = "Auto Generator Solution",
Default = false,
Callback = function(v)
AutoPrizeGenerator(v)
end})
KillerSurvival:AddDivider()
KillerSurvival:AddToggle("MyToggle",{
Text = "Auto Eat Pizza",
Callback = function(v)
if v then
for _,v in ipairs(workspace.Map.Ingame:GetDescendants()) do
if v:IsA("BasePart") and v.Name == "Pizza" then
if workspace:FindFirstChild("Map"):FindFirstChild("Ingame"):FindFirstChild(v) then
v.CFrame = _hrp.CFrame
end
end
end
AutoEatPizza = workspace.Map.Ingame.DescendantAdded:Connect(function(v)
if v:IsA("BasePart") and v.Name == "Pizza" then
if workspace:FindFirstChild("Map"):FindFirstChild("Ingame"):FindFirstChild(v) then
v.CFrame = _hrp.CFrame
end
end
end)
else
AutoEatPizza:Disconnect()
end
end})
local Visual = Tabs.Main:AddRightGroupbox("Visual")
local LibESP = loadstring(game:HttpGet("https://raw.githubusercontent.com/ImamGV/Script/main/ESP"))()

Visual:AddToggle("MyToggle",{
Text = "Survival ESP",
Callback = function(v)
if v then
for _, v in ipairs(workspace.Players:GetDescendants()) do
if v.Parent.Name == "Survivors" and not v:FindFirstChild("Survival_ESP") then
LibESP:AddESP(v, "", Color3.fromRGB(0, 255, 0), 14, "Survival_ESP")
end
end
SurvivalESP = workspace.Players.DescendantAdded:Connect(function(v)
if v.Parent.Name == "Survivors" and not v:FindFirstChild("Survival_ESP") then
LibESP:AddESP(v, "", Color3.fromRGB(0, 255, 0), 14, "Survival_ESP")
end
end)
else
SurvivalESP:Disconnect()
LibESP:Delete("Survival_ESP")
end
end})
Visual:AddToggle("MyToggle",{
Text = "Killer ESP",
Callback = function(v)
if v then
for _, v in ipairs(workspace.Players:GetDescendants()) do
if v.Parent.Name == "Killers" and not v:FindFirstChild("Killer_ESP") then
LibESP:AddESP(v, "", Color3.fromRGB(255, 0, 0), 14, "Killers_ESP")
end
end
KillersESP = workspace.Players.DescendantAdded:Connect(function(v)
if v.Parent.Name == "Killers" and not v:FindFirstChild("Killers_ESP") then
LibESP:AddESP(v, "", Color3.fromRGB(255, 0, 0), 14, "Killers_ESP")
end
end)
else
KillersESP:Disconnect()
LibESP:Delete("Killers_ESP")
end
end})
Visual:AddToggle("MyToggle",{
Text = "Entity Killers ESP",
Callback = function(v)
if v then
for _, v in ipairs(workspace:GetDescendants()) do
